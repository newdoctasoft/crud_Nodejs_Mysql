<% include parcial/encabezado %>

    <div class="container">
        <div class="col-md-5">
            <div class="card-body">

                <form action="/insertar" method="POST">
                    <div class="form-group">
                        <input type="text" name="nombre" placeholder="INSERTE EL NOMBRE" class="form-control" autofocus>
                    </div>
                    <div class="form-group">
                        <input type="text" name="apellido" placeholder="INSERTE EL APELLIDO" class="form-control" autofocus>
                    </div>
                    <div class="form-group">
                        <input type="text" name="direccion" placeholder="INSERTE LA DIRECCION" class="form-control">
                    </div>
                    <div class="form-group">
                        <input type="text" name="email" placeholder="INSERTE EL CORREO" class="form-control">
                    </div>
                    <div class="form-group">
                        <input type="text" name="telefono" placeholder="INSERTE EL TELEFONO" class="form-control">
                    </div>

                    <button type="submit" class="btn btn-primary">
                        GUARDAR
                    </button>
                </form>
            </div>
        </div>
    </div>

    <% include parcial/pie %>